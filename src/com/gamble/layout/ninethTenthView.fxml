    <children>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="开启下注" GridPane.rowIndex="1" />
        <ToggleButton fx:id="enable" mnemonicParsing="false" onAction="#handleEnable" text="开启下注" GridPane.columnIndex="1" GridPane.rowIndex="1" />
        <ToggleButton fx:id="disable" mnemonicParsing="false" onAction="#handleDisable" text="关闭下注" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <Text fx:id="enableStatusText" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <Text fx:id="exText" strokeType="OUTSIDE" strokeWidth="0.0" text="请选择不投注的数字" GridPane.rowIndex="4" />
        <CheckBox fx:id="ex1" mnemonicParsing="false" onAction="#handleNumExEvent" text="1" GridPane.rowIndex="5" disable="true"/>
        <CheckBox fx:id="ex2" mnemonicParsing="false" onAction="#handleNumExEvent" text="2" GridPane.columnIndex="1" GridPane.rowIndex="5" disable="true"/>
        <CheckBox fx:id="ex3" mnemonicParsing="false" onAction="#handleNumExEvent" text="3" GridPane.columnIndex="2" GridPane.rowIndex="5" disable="true"/>
        <CheckBox fx:id="ex4" mnemonicParsing="false" onAction="#handleNumExEvent" text="4" GridPane.columnIndex="3" GridPane.rowIndex="5" disable="true"/>
        <CheckBox fx:id="ex5" mnemonicParsing="false" onAction="#handleNumExEvent" text="5" GridPane.columnIndex="4" GridPane.rowIndex="5" disable="true"/>
        <CheckBox fx:id="ex6" mnemonicParsing="false" onAction="#handleNumExEvent" text="6" GridPane.rowIndex="7" disable="true"/>
