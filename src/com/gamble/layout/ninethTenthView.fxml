                    <String fx:value="5" />
                    <String fx:value="6" />
                    <String fx:value="7" />
                    <String fx:value="8" />
                    <String fx:value="9" />
                </FXCollections>
            </items>
            <value>
                <String fx:value="7" />
            </value>
        </ComboBox>
        <Label fx:id="maxBetCountLabel" text="十选" GridPane.halignment="RIGHT" GridPane.rowIndex="3" visible="false"/>

