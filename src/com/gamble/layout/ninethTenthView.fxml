        <TextField fx:id="smpChip9" text="0" GridPane.columnIndex="3" GridPane.rowIndex="11" disable="true"/>
        <TextField fx:id="smpChip10" text="0" GridPane.columnIndex="4" GridPane.rowIndex="11" disable="true" />
