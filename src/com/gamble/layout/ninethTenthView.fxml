        <ComboBox fx:id="gambleMode" onAction="#handleModeChange" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="雪球模式" />
                    <String fx:value="普通模式" />
