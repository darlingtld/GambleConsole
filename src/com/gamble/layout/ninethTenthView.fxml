                    <String fx:value="探测模式" />
                    <String fx:value="连关模式" />
                </FXCollections>
            </items>
            <value>
                <String fx:value="雪球模式" />
            </value>
        </ComboBox>
        <Text fx:id="smartModeStatus" strokeType="OUTSIDE" strokeWidth="0.0" text="雪球模式" GridPane.columnIndex="2" GridPane.rowIndex="2" />
        <ComboBox fx:id="smartSwitch" onAction="#handleSmartSwitchChange" visible="false" GridPane.columnIndex="3" GridPane.columnSpan="2" GridPane.rowIndex="2">
            <items>
                <FXCollections fx:factory="observableArrayList">
                    <String fx:value="1,3,5-6,8,10 对跳" />
