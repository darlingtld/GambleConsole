<GridPane alignment="center" hgap="10" prefHeight="102.0" prefWidth="650.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gamble.NinethTenthController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
